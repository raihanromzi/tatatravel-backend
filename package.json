{
  "name": "tatatravel-backend",
  "version": "1.0.0",
  "description": "A REST API backend built with Express.js.",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "nodemon src/main.js",
    "test": "jest",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raihanromzi/tatatravel-backend.git"
  },
  "author": "Raihan Romzi Rakhman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/raihanromzi/tatatravel-backend/issues"
  },
  "homepage": "https://github.com/raihanromzi/tatatravel-backend#readme",
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/supertest": "^2.0.12",
    "babel-jest": "^29.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1",
    "supertest": "^6.3.3"
  }
}
